LIBRARY ieee;
USE ieee.std_logic_1164.all;

ENTITY decodificador_columna_verde IS
	PORT(	w	:IN STD_LOGIC_VECTOR(5 DOWNTO 0);
			En	:IN STD_LOGIC;
			y	:OUT STD_LOGIC_VECTOR(3 DOWNTO 0));
END decodificador_columna_verde;

ARCHITECTURE Behavior of decodificador_columna_verde IS
	SIGNAL Enw: STD_LOGIC_VECTOR(6 DOWNTO 0);
BEGIN
	Enw <=En&w;
	WITH Enw SELECT
		y<="0001" WHEN "1000000",
			"0001" WHEN "1000001",
			"0001" WHEN "1000010",
			"0001" WHEN "1000011",
			"0001" WHEN "1000100",
			"0001" WHEN "1000101",
			"0001" WHEN "1000110",
			"0001" WHEN "1000111",
			"0001" WHEN "1001000",
			"0011" WHEN "1001001",
			"0011" WHEN "1001010",
			"0011" WHEN "1001011",
			"0011" WHEN "1001100",
			"0011" WHEN "1001101",
			"0011" WHEN "1001110",
			"0011" WHEN "1001111",
			"0011" WHEN "1010000",
			"0111" WHEN "1010001",
			"0111" WHEN "1010010",
			"0111" WHEN "1010011",
			"0111" WHEN "1010100",
			"0111" WHEN "1010101",
			"0111" WHEN "1010110",
			"0111" WHEN "1010111",
			"0111" WHEN "1011000",
			"1111" WHEN "1011001",
			"1111" WHEN "1011010",
			"1111" WHEN "1011011",
			"1111" WHEN "1011100",
			"1111" WHEN "1011101",
			"1111" WHEN "1011110",
			"1111" WHEN "1011111",
			"1111" WHEN "1100000",
			
			"1111" WHEN OTHERS;
END Behavior;